{
    "extends": [
        "next/core-web-vitals"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "warn",
        "react/no-unescaped-entities": "off",
        "@typescript-eslint/no-explicit-any": "warn",
        "react/display-name": "off"
    }
}